  <category>
    <pattern>QT2</pattern>
    <template>
      <srai>
      <learn graph="ProgramQ" filename="aiml\test_suite\ProgramQ\TestSuite.aiml" />
      <learn graph="ProgramQ" filename="aiml\test_suite\ProgramQ\bot.xml" />
      <learn graph="ProgramQ" filename="aiml\test_suite\ProgramQ\vars.xml" />
