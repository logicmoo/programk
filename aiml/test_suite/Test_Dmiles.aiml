  <aiml graph="ProgramQ">
    <category>
      <pattern>QT3</pattern>
      <template graph="ProgramQ">
        <system>@load <bot name="~genotype"/>\test_suite\ProgramQ\TestSuite.xml</system>
      </template>
    </category>
  </aiml>

  <category>
    <pattern>PROGRAMQ</pattern>
    <template>
      <learn path="ProgramQ/"/>
    </template>
  </category>

  <category>
    <pattern>PROGRAMD</pattern>
    <template graph="ProgramD">
      <learn graph="ProgramD" dir="ProgramD/"/>
      <srai>@chgraph programd</srai>
      <srai>@load aiml\test_suite\ProgramD\AIML.xml</srai>
    </template>
  </category>

<!--

@say1 Can you be more specific?
yes
@say1 Can you be more specific?
no
@say <random><li>a</li><li>b</li></random>
-->

  <category>
    <pattern>YES</pattern>
    <that>CAN YOU BE *</that>
    <template>
      Then please do be <set name="will">
        <thatstar/>
      </set>
    </template>
  </category>

  <category>
    <pattern>NO</pattern>
    <that>CAN YOU BE *</that>
    <template>
      Then please try to be <set name="will">
        <thatstar/>
      </set>
    </template>
  </category>

  <category>
    <pattern>_</pattern>
    <pattern>*</pattern>
    <that>CAN YOU BE *</that>
    <template>
      So that meant maybe yes that you'll be <set name="will">
        <thatstar/>
      </set>
    </template>
  </category>

  <category>
    <pattern>YES</pattern>
    <that>SO THAT MEANS *</that>
    <template>
      Then <get name="will"/>
    </template>
  </category>

  <category>
    <pattern>NO</pattern>
    <that>SO THAT MEANS *</that>
    <template>
      why not <get name="will"/>?
    </template>
  </category>

